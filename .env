# GANTI nilai-nilai ini lalu simpan sebagai .env (jangan commit token asli ke repo publik!)
TELEGRAM_BOT_TOKEN="REPLACE_WITH_YOUR_TOKEN"
TELEGRAM_CHAT_ID="YOUR TELEGRAM USEER ID"

# file log yang dipantau (bisa banyak, pisah koma)
LOG_FILES="/root/cysic-prover/cysic-prover.log"

# pola 'Received Task' + blackout
KEYWORDS='re:start prepare task:\s*\d+'

# pola yang diforward mentah (tanpa judul, tanpa blackout)
RAW_ONLY_PATTERNS='re:submit\s+taskData,\s*task:\s*\d+.*,re:task:\s*\d+\s+process\s+submitProofData\s+finish'

# durasi blackout setelah trigger KEYWORDS (detik)
BLACKOUT_SECONDS="300"
